{
    "name": "tobor",
    "version": "1.0.0",
    "description": "cmdjwj's telegram bot",
    "main": "index.ts",
    "scripts": {
        "postinstall": "chmod +x ./scripts/webhook.sh",
        "set-webhook": "./scripts/webhook.sh setup",
        "vercel-build": "npm run clean && npm run build && npm run set-webhook",
        "build": "tsc",
        "start": "ts-node index.ts",
        "dev": "ts-node-dev index.ts",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "dotenv": "^16.4.7",
        "grammy": "^1.34.0",
        "https-proxy-agent": "^7.0.6"
    },
    "devDependencies": {
        "@vercel/node": "^5.0.2",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.7.3"
    },
    "engines": {
        "node": "22.x"
    },
    "keywords": [
        "telegram",
        "bot",
        "grammy"
    ],
    "author": "cmdjwj",
    "license": "MIT"
}